{
    "name": "msgext-github",
    "version": "1.0.0",
    "description": "Microsoft Teams Toolkit m365 message extension sample",
    "engines": {
        "node": "16 || 18"
    },
    "author": "Microsoft",
    "license": "MIT",
    "main": "./lib/index.js",
    "scripts": {
        "dev:teamsfx": "env-cmd --silent -f .localConfigs npm run dev",
        "dev": "nodemon --exec node --inspect=9239 --signal SIGINT -r ts-node/register ./index.ts",
        "build": "tsc --build",
        "start": "node ./lib/index.js",
        "storage": "azurite --silent --location ./_storage_emulator --debug ./_storage_emulator/debug.log",
        "test": "echo \"Error: no test specified\" && exit 1",
        "watch": "nodemon --exec \"npm run start\""
    },
    "repository": {
        "type": "git",
        "url": "https://github.com"
    },
    "dependencies": {
        "@azure/data-tables": "^13.2.2",
        "adaptive-expressions": "^4.20.1",
        "adaptivecards": "^3.0.1",
        "adaptivecards-templating": "^2.3.1",
        "botbuilder": "^4.20.0",
        "restify": "^10.0.0"
    },
    "devDependencies": {
        "@types/node": "^14.0.0",
        "@types/restify": "^8.5.5",
        "azurite": "3.26.0",
        "env-cmd": "^10.1.0",
        "nodemon": "^2.0.7",
        "shx": "^0.3.3",
        "ts-node": "^10.4.0",
        "typescript": "^4.4.4"
    }
}
